    "@types/jest": "^26.0.15",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "eslint": "^7.14.0",
    "jest": "^26.6.3",
    "typescript": "^4.1.2"
  },
  "jest": {
    "testMatch": [
      "**/*.test.js"
